$('#loading_products').remove();
$('#admin-products-container table tbody').append('<%= j render partial: "admin/product", collection: @products %>');
<% if @products.next_page %>
  $('.pagination').replaceWith('<%= j will_paginate @products %>');
<% else %>
  $(window).off('scroll');
  $('.pagination').remove();
<% end %>

initialize_modals();